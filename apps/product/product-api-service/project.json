{
    "name": "product-api-service",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/product/product-api-service/src",
    "projectType": "application",
    "tags": [],
    "targets": {
    "build-serverless-files": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dist/apps/product/product-api-service",
        "commands": [
          {
            "command": "yarn add tslib"
          }
        ],
        "parallel": false
      }
    },
        "serve": {
            "executor": "@nx/js:node",
            "defaultConfiguration": "development",
            "dependsOn": ["build"],
            "options": {
                "buildTarget": "product-api-service:build",
                "runBuildTargetDependencies": false
            },
            "configurations": {
                "development": {
                    "buildTarget": "product-api-service:build:development"
                },
                "production": {
                    "buildTarget": "product-api-service:build:production"
                }
            }
        }
    }
}
